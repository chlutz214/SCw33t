        {
            "matches": [
                "https://ca.my.salesforce.com/liveagent/supervisor.apexp*"
            ],
            "exclude_globs": [
                "*dontscw33ten=1*"
            ],
            "match_about_blank": false,
            "all_frames": true,
            "run_at": "document_end",
            "js": [
                "live_agent_supervisor_content.js"
