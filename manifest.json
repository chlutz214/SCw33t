            "all_frames": true,
            "run_at": "document_end",
            "js": [
                "email_content.js"
            ]
        },
        {
            "matches": [
                "https://ca--c.na13.visual.force.com/apex/defect?caseId=*"
            ],
            "exclude_globs": [
                "*dontscw33ten=1*"
            ],
            "match_about_blank": false,
            "all_frames": true,
